
enum Category {  
    SPORTS,
    ACADEMIC,
    CULTURAL,
    SOCIAL,
    PROFESSIONAL,
    OTHER
}

export class Club {
  id: number;
  category: Category;
  name: string;
  description: string;
  president: User;
  members: User[];

  constructor(
    id: number,
    category: Category,
    name: string,
    description: string,
    president: User,
    members: User[]
  ) {
    this.id = id;
    this.category = category;
    this.name = name;
    this.description = description;
    this.president = president;
    this.members = members;
  }
}
